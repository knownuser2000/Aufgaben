set xlabel "Zeit-Achse"
 #Achsenbeschriftung
set ylabel "Konzentrationen-Achse" 
#Achsenbeschriftung
set title "Euler semi implizit" 
plot "Euler_semi_implizit.txt" using 1:2 title "c_A" with l
